{
  "manifest_version": 3,
  "default_locale": "en",
  "name": "__MSG_extName__",
  "version": "1.0",
  "description": "__MSG_extDescription__",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "https://*.jut.su/*"
  ],
  "background": {
    "service_worker": "/src/background/background.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "128": "/src/assets/icon128.png"
    },
    "default_title": "JutSuper",
    "default_popup": "/src/popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.jut.su/*"
      ],
      "js": [
        "/src/content/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/src/log.js",
        "/src/consts.js",
        "/src/error.js",
        "/src/util.js",
        "/src/storage.js",
        "/src/ipc.js",
        "/src/messaging.js",
        "/src/browser.js",
        "/src/page/jutsuper.js",
        "/src/page/jutsuper.css",
        "/src/assets/gear.svg"
      ],
      "matches": [
        "https://*.jut.su/*"
      ]
    }
  ],
  "icons": {
    "128": "/src/assets/icon128.png"
  }
}